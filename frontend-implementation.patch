+              href={link.url}
+              target="_blank"
+              rel="noopener noreferrer"
+              className="flex items-center gap-2 text-blue-500 hover:text-blue-600 transition-colors duration-200"
+            >
+              <ExternalLink className="w-4 h-4" />
+              
+            </a>
