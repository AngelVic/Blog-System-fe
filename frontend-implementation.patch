+
+interface LinkListProps {
+  links: Link[];
+  isLoading?: boolean;
+  onDelete: (id: number) => Promise<void>;
+}
+
+const LinkList: FC<LinkListProps> = ({ links, isLoading = false, onDelete }) => {
+  if (isLoading) {
+    return (
+      <div className="space-y-4">
+        {[1, 2, 3].map((i) => (
+          <Card key={i} className="animate-pulse">
+            <CardContent className="p-6">
